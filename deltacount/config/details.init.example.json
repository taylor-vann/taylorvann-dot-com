{
  "server": {
    "https_port": 443,
    "read_timeout": 5,
    "write_timeout": 10,
    "idle_timeout": 5
  },
  "cert_paths": {
    "cert": "/usr/local/config/fullchain.pem",
    "private_key": "/usr/local/config/privkey.pem"
  },
  "limiter": {
    "interval": 1,
    "maximum_server_requests": 10000
  },
  "cache": {
    "host": "127.0.0.1",
    "idle_timeout": 1,
    "max_active": 1024,
    "max_idle": 128,
    "port": 4567,
    "protocol": "tcp"
  }
}
